@model PagerModel

<div class="container m-0 p-0 btn-group" style="background-color:rgb(65,152,199); border-radius:10px height:45px; color:white;">
    <div class="col-4 btn-group p-1 m-1">
        <p>
            Showing @Model.StartRecord to @Model.EndRecord of @Model.TotalItems Rows
        </p>
    </div>
    <div class="col-7 btn-group justify-content-end m-1">
        <p>
            Showing @Model.CurrentPage/@Model.TotalPages Rows 
        </p>
    </div>
</div>
